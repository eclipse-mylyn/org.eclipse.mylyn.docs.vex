              <box class="LineBox">
                <box class="DocumentTextBox" text="newline &#xa;" />
              </box>
              <box class="LineBox">
                <box class="DocumentTextBox" text=" " />
                <box class="DocumentTextBox" text="end" />
                <box class="PlaceholderBox"/>
              </box>
            </box>

          </box>
        </box>
      </box>
    </result>
  </test>
  
  
